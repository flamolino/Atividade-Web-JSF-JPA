<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>go-to-index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>go-to-institutions</from-outcome>
            <to-view-id>/pages/institutions.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>go-to-courses</from-outcome>
            <to-view-id>/pages/courses.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>go-to-halfs</from-outcome>
            <to-view-id>/pages/halfs.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>go-to-teachers</from-outcome>
            <to-view-id>/pages/teachers.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>go-to-students</from-outcome>
            <to-view-id>/pages/students.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>go-to-curriculums</from-outcome>
            <to-view-id>/pages/curriculums.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>go-to-disciplines</from-outcome>
            <to-view-id>/pages/disciplines.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>go-to-notations</from-outcome>
            <to-view-id>/pages/notations.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>go-to-frequencies</from-outcome>
            <to-view-id>/pages/frequencies.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>